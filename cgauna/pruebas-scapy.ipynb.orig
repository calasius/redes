    "def simbolo_s(package):\n",
    "    if p.dst == 'ff:ff:ff:ff:ff:ff':\n",
    "        return BROADCAST\n",
    "    else:\n",
    "        return UNICAST"
   ]
  },
  {
   "cell_type": "code",
